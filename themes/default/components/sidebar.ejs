<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/nanobar/0.4.2/nanobar.js"></script>
<style>
   .loadingbar .bar {
   background: #4399fa;
   height: 2px;
   }
</style>
<style>
   .spinner {
   position: fixed;
   top: 10px; /* Change top to bottom */
   right: 10px;
   width: 40px;
   border: 4px solid #0f1012;
   border-radius: 50%;
   border-top: 4px solid white;
   width: 26px;
   height: 26px;
   -webkit-animation: spin 0.5s linear infinite;
   /* Safari */
   animation: spin 0.5s linear infinite;
   opacity: 0;
   transition: opacity 0.5s;
   }
   /* Safari */
   @-webkit-keyframes spin {
   0% {
   -webkit-transform: rotate(0deg);
   }
   100% {
   -webkit-transform: rotate(360deg);
   }
   }
   @keyframes spin {
   0% {
   transform: rotate(0deg);
   }
   100% {
   transform: rotate(360deg);
   }
   }
</style>
<script>
   document.addEventListener("DOMContentLoaded", function () {
       var currentPageUrl = window.location.href;
   
       // Get all nav links
       var navLinks = document.querySelectorAll('.nav-link');
   
       // Loop through each
       navLinks.forEach(function (link) {
           // Get the href of the link
           var linkUrl = link.getAttribute('href');
   
           // Check if the current page URL contains the active URL
           if (currentPageUrl.includes(linkUrl.replace('../', '/'))) {
               // Remove gray text
               link.classList.remove('text-zinc-400');
   
               // Make it active
               link.classList.add(
                   'transition',
                   'text-white',
               );
           }
       });
   });
</script>
<script>
   (function() {
     function displayWarning() {
       console.clear();
   console.log(` LEXACTYL `);
       console.log(' ');
       console.log('%c⚠️ Warning', 'color: orange; font-weight: bold;');
       console.log('Pasting anything here may result in someone getting access to your account!');
       console.log('Please be cautious and avoid pasting anything that could be malicious.');
       console.log(' ');
     }
   
     function startWarningInterval() {
       displayWarning();
       setInterval(displayWarning, 10000); // every 10 seconds
     }
   
     startWarningInterval();
   })();
</script>
<script>
   console.log('%c[SYSTEM] Starting page render...', 'color: #60a5fa; font-weight: bold;');
   var options = {
       classname: 'loadingbar',
       id: 'loadingbar'
   };
   var nanobar = new Nanobar(options);
   var spinner = document.getElementById('spinner');
   spinner.style.opacity = '1';
   function simulateLoading() {
       nanobar.go(10); // Change this value to control the loading speed
       setTimeout(function() {
           nanobar.go(50);
       }, 190);
       setTimeout(function() {
           nanobar.go(100);
           // Hide the spinner with a smooth transition
           spinner.style.opacity = '0';
       }, 400);
   }
   simulateLoading();
</script>
<script>
   setTimeout(function() {console.log('%c[SYSTEM] Page render complete!', 'color: #60a5fa; font-weight: bold;'); }, 800); 
   setTimeout(function() {console.log('%c[SYSTEM] Lightning has finished all tasks', 'color: aqua; font-weight: aqua;'); }, 1200); 
</script>
